{
  "name": "react-responsive-audio-player",
  "version": "1.3.2",
  "description": "A simple, clean, and responsive visual wrapper for the HTML audio tag, built with React.",
  "main": "dist/audioplayer.js",
  "scripts": {
    "build:clean": "rimraf dist",
    "build:webpack": "BUILD_MODE=all webpack --progress",
    "build:component_css": "gulp styles",
    "build": "npm run build:clean && npm run build:webpack && npm run build:component_css",
    "dev": "BUILD_MODE=unminimized webpack-serve --open --open-path example.html",
    "dev:network": "BUILD_MODE=unminimized webpack-serve --host 0.0.0.0 --open --open-path example.html",
    "prepublish": "npm run build",
    "prettier": "pretty-quick --staged \"**/*.js\"",
    "prettier-check": "node prettier-check.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-check"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benwiley4000/react-responsive-audio-player.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "audio",
    "html5",
    "react",
    "reactjs",
    "responsive",
    "music",
    "player"
  ],
  "author": {
    "name": "Ben Wiley",
    "email": "therealbenwiley@gmail.com",
    "url": "http://benwiley.org/"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.41",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.41",
    "@babel/preset-env": "^7.0.0-beta.41",
    "@babel/preset-react": "^7.0.0-beta.41",
    "@benwiley4000/svg-react-loader": "^0.4.5-fix-vulnerabilities",
    "array-find-index": "^1.0.2",
    "autoprefixer": "^7.1.1",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.4",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-uglifycss": "^1.0.8",
    "husky": "^1.0.0-rc.14",
    "icono": "^1.3.1",
    "material-design-icons": "^3.0.1",
    "mini-css-extract-plugin": "^0.2.0",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.5",
    "prettier": "1.14.2",
    "pretty-quick": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.5",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "resize-observer-polyfill": "^1.4.2"
  }
}
