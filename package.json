{
  "name": "react-responsive-audio-player",
  "version": "1.3.0",
  "description": "A simple, clean, and responsive visual wrapper for the HTML audio tag, built with React.",
  "main": "dist/audioplayer.js",
  "scripts": {
    "build:webpack": "rimraf dist && webpack --progress",
    "build:minify:js": "uglifyjs dist/audioplayer.js -o dist/audioplayer.min.js",
    "build:minify:css": "cleancss dist/audioplayer.css -o dist/audioplayer.min.css",
    "build:minify": "npm run build:minify:js && npm run build:minify:css",
    "build:component_css": "gulp styles",
    "build": "npm run build:webpack && npm run build:minify && npm run build:component_css",
    "dev": "webpack-dev-server --open",
    "dev:network": "webpack-dev-server --host 0.0.0.0 --disable-host-check --open",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benwiley4000/react-responsive-audio-player.git"
  },
  "engines": {
    "node": "*"
  },
  "keywords": [
    "audio",
    "html5",
    "react",
    "reactjs",
    "responsive",
    "music",
    "player"
  ],
  "author": {
    "name": "Ben Wiley",
    "email": "therealbenwiley@gmail.com",
    "url": "http://benwiley.org/"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.3.0-alpha.0"
  },
  "devDependencies": {
    "array-find-index": "^1.0.2",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "classnames": "^2.2.5",
    "clean-css-cli": "^4.1.3",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.3.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-uglifycss": "^1.0.8",
    "icono": "^1.3.1",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "react-lifecycles-compat": "^1.0.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "svg-react-loader": "^0.4.0-beta.2",
    "uglify-js": "^3.0.10",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "resize-observer-polyfill": "^1.4.2"
  }
}
